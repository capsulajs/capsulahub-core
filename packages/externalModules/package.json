{
  "name": "@capsulajs/capsulahub-core-external-modules",
  "version": "0.0.1",
  "description": "An external modules package for CapsulaHub core services",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "license": "MIT",
  "private": false,
  "scripts": {
    "run-workspace-55555": "capsula-hub run -l ./workspace-config-5555.js -p 55555",
    "run-workspace-7777": "capsula-hub run -l ./workspace-config-7777.js -p 7777",
    "run-workspace-4444": "capsula-hub run -l ./workspace-config-4444.js -p 4444",
    "run-workspace-8888": "capsula-hub run -l ./workspace-config-8888.js -p 8888",
    "start": "node server.js",
    "build-example": "webpack --config webpack.config-example.js",
    "start-example": "http-server -p 9999",
    "build": "webpack",
    "lint": "yarn lint:md && yarn lint:ts",
    "lint:md": "remark . --rc-path ../../.remarkrc --output",
    "lint:ts": "tslint 'src/**/*.{ts,tsx}' --fix",
    "prettier": "prettier --write 'src/**/*.{ts,tsx}'",
    "clean": "rimraf public node_modules"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@capsulajs/capsulahub-ui": "^0.6.2",
    "@capsulajs/environment-registry": "^0.0.2",
    "@types/react": "^16.8.18",
    "@types/react-dom": "^16.8.4",
    "express": "^4.16.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "rxjs": "^6.5.2"
  },
  "devDependencies": {
    "@capsulajs/capsula-hub": "^0.0.4"
  }
}
